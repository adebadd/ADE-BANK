    private void jbDeleteActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbDeleteActionPerformed
       String numfordelete = jtfdelete.getText();
       if (Customer.customerexists3(numfordelete)){
            Customer foundCustomerAccountNum = Customer.findCustomerAccountNum(numfordelete);
            AccountTransactions foundAccount = AccountTransactions.findAccount(numfordelete);
            
            if (foundAccount != null){
                Double checkbalance = 0.00;
            if (checkbalance != foundAccount.getCustomerBalance()){
            new ErrorCannotDelete().setVisible(true);
            
            } else {
            foundAccount.setTransactionAmount(0.00);
            foundAccount.setcustomerAccount("Account Deleted");
             foundAccount.setTransactionType("Account Deleted");
             foundAccount.setTransactionDate("Account Deleted");
             foundAccount.setcustomerBalance(0.00);
             
             foundCustomerAccountNum.setcustomerPass("Account Deleted");
             foundCustomerAccountNum.setaccountNum("Account Deleted");
             foundCustomerAccountNum.setcustomerFirst("Account Deleted");
             foundCustomerAccountNum.setcustomerLast("Account Deleted");
             foundCustomerAccountNum.setcustomerPPSN("Account Deleted");
             foundCustomerAccountNum.setcustomerID("Account Deleted");
             foundCustomerAccountNum.setcustomerDOB("Account Deleted");
            }
            
            
            }
            
       } else {
       new ErrorAccountNotFound().setVisible(true);   
       }
       
       
       
       
       
       
    }//GEN-LAST:event_jbDeleteActionPerformed

    private void lbllogoutMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_lbllogoutMouseClicked
       if (lbllogout.isEnabled()){
         lblbalance.setCursor(Cursor.getPredefinedCursor(Cursor.HAND_CURSOR));
       lblbalance.setEnabled(false);
    	lbllogement.setEnabled(false);
    	 lblviewdetails.setEnabled(false);
         lblwithdraw.setEnabled(false);
    	lbllogin.setEnabled(true);
        lbllogout.setEnabled(false);
        lblcreate.setEnabled(true);
       
       }
    }//GEN-LAST:event_lbllogoutMouseClicked

